{
  "production": {
    "mongodb": "reservalia-db-00:27017,reservalia-db-01:27017,reservalia-db-02:27017/api",
    "startupMessage": "AUTH SERVICE NOT READY, RETURN IN FEW SECONDS!",
    "redisConnection": [
        {"host": "reservalia-db-00", "port": 26379},
        {"host": "reservalia-db-01", "port": 26379},
        {"host": "reservalia-db-02", "port": 26379}
    ],
    "collections": [
      "tokens",
      "keys"
    ],
    "refreshKeys": 10,
    "rateLimit": 100,
    "base": "/",
    "excludes": [
      "/docs","/swagger.json","/docsui","/jsdocs","/swagger","/css","/lib","/favicon.ico"
    ],
    "forcelogin": false
  }
}
